{
    "name": "buybrain/nervus-php",
    "version": "0.0.3",
    "description": "A PHP library for creating nervus adapters",
    "keywords": [
        "nervus"
    ],
    "homepage": "https://github.com/buybrain/nervus-php",
    "type": "library",
    "licence": "MIT",
    "authors": [
        {
            "name": "Ede Meijer",
            "email": "ede@buybrain.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Buybrain\\": "src/Buybrain/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Buybrain\\": "test/Buybrain/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "require": {
        "rybakit/msgpack": "^0.2.1"
    },
    "suggest": {
        "msgpack/msgpack-php": "Native MessagePack codec that's more efficient than the default pure PHP version"
    }
}
